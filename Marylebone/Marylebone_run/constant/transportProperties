// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  Newtonian;

nu              [0 2 -1 0 0 0 0] 1.5e-05;

DT              DT [0 2 -1 0 0 0 0] 0.0;

SchmidtTurbulent         0.7;
solveMomentum           true;
solveScalar             true;

